    "@vueneue/ssr-core": "^0.3.21",
    "@vueneue/ssr-server": "^0.3.21",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.2",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-plugin-cypress": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "hygen": "^1.6.2",
    "koa-compress": "^3.0.0",
    "lodash": "^4.17.11",
    "node-sass": "^4.9.3",
    "raven-js": "^3.27.0",
    "register-service-worker": "^1.5.2",
    "sass-loader": "^7.1.0",
    "start-server-and-test": "^1.7.7",
    "vue": "^2.5.17",
    "vue-jest": "3.0.0",
    "vue-meta": "^1.5.3",
    "vue-progressbar": "^0.7.5",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@vue/cli-plugin-babel": "^3.0.x",
    "@vue/cli-plugin-e2e-cypress": "^3.0.x",
    "@vue/cli-plugin-eslint": "^3.0.x",
    "@vue/cli-plugin-pwa": "^3.0.x",
    "@vue/cli-plugin-unit-jest": "^3.0.x",
    "@vue/cli-service": " ^3.0.x",
    "@vue/eslint-config-prettier": "^3.0.x",
    "@vueneue/vue-cli-plugin-ssr": "^0.3.22",
    "babel-jest": "23.6.x",
    "lint-staged": "^8.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
